{{& labeled_scores_html }}

{{#replaced_by}}
<h3 class="title">Suggested replacement</h3>
<p>
  <a href="{{& replaced_by_link }}" title="This package is discontinued, but author has suggested package:{{ replaced_by }} as a replacement">{{ replaced_by }}</a>
</p>
{{/replaced_by}}

{{#has_publisher}}
<h3 class="title">Publisher</h3>
<p>
  <a href="{{& publisher_link}}"><img
          class="-pub-publisher-shield"
          title="Published by a pub.dev verified publisher"
          src="{{{static_assets.img__verified-publisher-blue_svg}}}" />{{publisher_id}}</a>
</p>
{{/has_publisher}}

<h3 class="title pkg-infobox-metadata">Metadata</h3>
{{#description}}
  <p>{{description}}</p>
{{/description}}
<p>
  {{#meta_links}}
  <a class="link" href="{{& href}}"{{#rel}} rel="{{ rel }}"{{/rel}}>{{label}}</a><br/>
  {{/meta_links}}
</p>

{{#has_doc_links}}
<h3 class="title">Documentation</h3>
<p>
  {{#doc_links}}
    <a class="link" href="{{& href}}"{{#rel}} rel="{{ rel }}"{{/rel}}>{{label}}</a><br/>
  {{/doc_links}}
</p>
{{/has_doc_links}}

{{#license_html}}
<h3 class="title">License</h3>
<p>{{& license_html}}</p>
{{/license_html}}

{{#dependencies_html}}
<h3 class="title">Dependencies</h3>
<p>{{& dependencies_html}}</p>
{{/dependencies_html}}

<h3 class="title">More</h3>
<p><a href="{{{search_deps_link}}}" rel="nofollow">Packages that depend on {{name}}</a></p>
