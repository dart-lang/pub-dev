<div class="home-lists-container">
  <div class="landing-page-title-block">
    <div class="tooltip-base hoverable">
      <h2 class="center landing-page-title tooltip-dotted">{{top_header}}</h2>
      <div class="tooltip-content">
        {{& ranking_tooltip_html }}
      </div>
    </div>
  </div>
  {{& top_html}}
  <div class="more">
    {{more_packages}}
    <a href="{{{packages_url}}}">top</a>,
    <a href="{{{updated_url}}}">updated</a>,
    <a href="{{{created_url}}}">created</a>.
  </div>
</div>
